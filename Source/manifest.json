{
  "manifest_version": 3,
  "name": "Abbrevihater",
  "version": "0.0.0.1",
  "description": "Replaces abbreviations/acronyms with the full terms.",
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "all_frames": true,
      "js": ["content_script.js"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
